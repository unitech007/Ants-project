<% include ../partials/header %>

    <title><%= __('common.admin_enquiry_title') %></title>
    <link rel="stylesheet" href="/stylesheets/enquiry.css" />

<% include ../partials/navbar %>
<div class="enquiries-list-container">
    <h1><%= __('common.admin_enquiry_heading') %></h1>
    <table border="1">
        <thead>
            <tr>
                <th>#</th>
                <th><%= __('common.admin_enquiry_name') %></th>
                <th><%= __('common.admin_enquiry_email') %></th>
                <th><%= __('common.admin_enquiry_mobile') %></th>
                <th><%= __('common.admin_enquiry_subject') %></th>
                <th><%= __('common.admin_enquiry_message') %></th>
                <th><%= __('common.admin_enquiry_date') %></th>
            </tr>
        </thead>
        <tbody>
            <% enquiries.forEach((enquiry, index) => { %>
                <tr>
                    <td><%= index + 1 %></td>
                    <td><%= enquiry.name %></td>
                    <td><%= enquiry.email %></td>
                    <td><%= enquiry.mobile %></td>
                    <td><%= enquiry.subject %></td>
                    <td><%= enquiry.message %></td>
                    <td><%= enquiry.createdAt.toDateString() %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>
<% include ../partials/footer %>
