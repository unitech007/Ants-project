   <% include ../partials/header %>

<meta name="description" content="<%= __('common.vendors_list_meta') %>">
<title><%= __('common.vendors_list_title') %></title>
<link rel="stylesheet" href="/stylesheets/v_list.css" />
<link rel="stylesheet" href="/stylesheets/image_style.css" />

<% include ../partials/navbar %>

<div class="text-center container vendors-list-container">
    <div class="row">
        <% foundVendors.forEach(function(vendor){ %>
        <div class="col-xl-3 col-lg-3 col-md-6 col-sm-6 col-12">
            <div class="vendor-image-div">
                <div class="vendor-image"><img src="<%= vendor.image %>" alt=""></div>
            </div>
            <h2 class="vendor-fname"><%= vendor.fname %></h2>
            <h5 class="vendor-lname"><%= vendor.lname %></h5>
            <span class="vendor-rating"><%= __('common.vendor_rating') %></span>
            <p><a href="<%= vendor.subservice %>/<%= vendor._id %>" class="btn btn-info"><%= __('common.more_info_button') %></a></p>
        </div>
        <% }); %>
    </div>
</div>

<% include ../partials/footer %>   